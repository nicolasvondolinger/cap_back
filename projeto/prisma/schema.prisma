datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id          Int @id @default(autoincrement())
  email       String  @unique
  name        String
  password    String
  photo       String?
  role        String
}

model Artist{
  id Int @id @default(autoincrement())
  name String
  photo Boolean
  streams Int @default(0)
}

model Musics{
  id Int @id @default(autoincrement())
  name String
  gender String
  album String
}